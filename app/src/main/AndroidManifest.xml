<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthcare">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Dr. ML"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".obeseActivity"></activity>
        <activity android:name=".overweightActivity" />
        <activity android:name=".healthyActivity" />
        <activity android:name=".thinActivity" />
        <activity android:name=".bmiActivity" />
        <activity android:name=".clickPictureActivity" />
        <activity android:name=".uploadPictureActivity" />
        <activity android:name=".EmergencyActivity" />
        <activity android:name=".HealthActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>